{
  "name": "@synappsagency/gabl-api",
  "version": "1.0.0",
  "description": "GABL app GraphQL API + some other functions",
  "main": "index.js",
  "author": "Yurii Sichkovskiy <filingr000ve@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "nodemon index.ts",
    "seed": "./seed.sh",
    "build": "tsc",
    "start:script": "nodemon --exec 'ts-node index.ts'",
    "start:prod": "cp .env ./dist/.env && cp config/firebase-config.json dist/config/firebase-config.json && node dist/index.js",
    "helper": "cd helpers && node",
    "gen": "cat ./common.gql ./**/**/*.gql > ./schema.gql && graphql-codegen --overwrite --config codegen.yml",
    "gen:watch": "cat ./common.gql ./**/**/*.gql > ./schema.gql && graphql-codegen --watch --overwrite --config codegen.yml"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.3.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-document-nodes": "2.3.3",
    "@graphql-codegen/typescript-mongodb": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@graphql-tools/schema": "^8.3.11",
    "apollo-datasource-mongodb": "^0.2.8",
    "apollo-server-express": "^3.7.0",
    "axios": "^0.27.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.6.0",
    "firebase-dynamic-links": "^1.1.1",
    "googleapis": "^92.0.0",
    "graphql": "^15.4.0",
    "graphql-fields-list": "^2.2.1",
    "graphql-scalars": "^1.6.1",
    "in-app-purchase": "^1.11.4",
    "lodash": "^4.17.20",
    "lru-cache": "^7.10.1",
    "luxon": "^2.2.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.5",
    "mongoose-autopopulate": "^0.12.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "node-schedule": "^2.1.0",
    "open-graph-scraper": "^4.11.0",
    "sift": "^13.5.0",
    "stripe": "^9.9.0",
    "twilio": "^3.77.1",
    "validator": "^13.7.0",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "@graphql-tools/schema": "^9.0.4",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.2",
    "@typescript-eslint/parser": "^5.37.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/utils": "^5.37.0",
    "@types/node": "^18.7.18",
    "hplipsum": "^1.0.0",
    "https": "^1.0.0",
    "jabber": "^1.2.2",
    "nodemon": "^2.0.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  }
}
